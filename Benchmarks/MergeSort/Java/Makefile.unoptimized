-include ../Makefile.defaults

compile:
	javac -d . Unoptimized.java

clean:
	rm Unoptimized.class

measure:
ifndef JSON
	$(error No JSON defined.)
else ifndef ARGS
	$(error No ARGS defined.)
endif
	@../../../RAPL/build/rapl --json "$(JSON)" java Unoptimized $(ARGS)

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo java Unoptimized $(ARGS)