-include ../Makefile.defaults

compile:
	javac -d . Optimized.java

clean:
	rm Optimized.class

measure:
ifndef JSON
	$(error No JSON defined.)
else ifndef ARGS
	$(error No ARGS defined.)
endif
	@../../../RAPL/build/rapl --json "$(JSON)" java Optimized $(ARGS)

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo java Optimized $(ARGS)