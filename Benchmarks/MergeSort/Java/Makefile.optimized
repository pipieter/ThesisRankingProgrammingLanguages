-include ../Makefile.defaults

compile:
	javac -d . MergeSort.java

clean:
	rm MergeSort

measure:
ifndef JSON
	$(error No JSON defined.)
else ifndef ARGS
	$(error No ARGS defined.)
endif
	@../../../RAPL/build/rapl --json "$(JSON)" java MergeSort $(ARGS)

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo java MergeSort $(ARGS)