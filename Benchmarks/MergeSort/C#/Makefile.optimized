-include ../Makefile.defaults

compile:
	dotnet build -o build/optimized/ -c Release --no-incremental

clean:
	dotnet clean

measure:
ifndef JSON
	$(error No JSON defined.)
else ifndef ARGS
	$(error No ARGS defined.)
endif
	@../../../RAPL/build/rapl --json "$(JSON)" build/optimized/merge-sort $(ARGS)

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo build/optimized/merge-sort $(ARGS)
