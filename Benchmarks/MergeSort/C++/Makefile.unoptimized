-include ../Makefile.defaults

compile:
	g++ unoptimized.cpp -o unoptimized -O3

clean:
	rm unoptimized

measure:
ifndef JSON
	$(error No JSON defined.)
else ifndef ARGS
	$(error No ARGS defined.)
endif
	@../../../RAPL/build/rapl --json "$(JSON)" ./unoptimized $(ARGS)

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo ./unoptimized $(ARGS)