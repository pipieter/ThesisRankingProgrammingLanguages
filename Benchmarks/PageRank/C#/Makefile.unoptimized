-include ../Makefile.defaults

compile:
	dotnet build unoptimized.csproj -o build/unoptimized -c Release --no-incremental

clean:
	dotnet clean

measure:
ifndef JSON
	$(error No JSON defined.)
else ifndef ARGS
	$(error No ARGS defined.)
endif
	@../../../RAPL/build/rapl --json "$(JSON)" ./build/unoptimized/unoptimized $(ARGS)

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo ./build/unoptimized/unoptimized $(ARGS)
