-include ../Makefile.defaults

compile:
	mkdir -p ./out/gcc
	g++ C++/unoptimized.cpp -o ./out/gcc/unoptimized -O3
	g++ C++/optimized.cpp   -o ./out/gcc/optimized   -O3

command:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo ./out/gcc/unoptimized $(ARGS)

command-optimized:
ifndef ARGS
	$(error No ARGS defined.)
endif
	@echo ./out/gcc/optimized $(ARGS)